        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test_application"
        mc:Ignorable="d"
        Title="MainWindow" Height="449" Width="800"
        Loaded="Window_Loaded"
        >
    <Grid>
        <TextBox HorizontalAlignment="Center" Name="textBox_name" Margin="0,299,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <DataGrid Margin="0,0,0,216" Name="DataGrid1" AutoGenerateColumns="True"></DataGrid>
        <TextBox HorizontalAlignment="Center" x:Name="textBox_email" Margin="0,266,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
