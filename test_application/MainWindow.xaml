        <Label Content="Регистрация" HorizontalAlignment="Center" Margin="0,229,0,0" VerticalAlignment="Top" Width="140" HorizontalContentAlignment="Center"/>
        <Label Content="name:" HorizontalAlignment="Left" Margin="281,263,0,0" VerticalAlignment="Top" Width="46" HorizontalContentAlignment="Center" Height="23"/>
        <Label Content="age:" HorizontalAlignment="Left" Margin="281,326,0,0" VerticalAlignment="Top" Width="46" HorizontalContentAlignment="Center" Height="27"/>
        <Label Content="email:" HorizontalAlignment="Left" Margin="281,296,0,0" VerticalAlignment="Top" Width="46" HorizontalContentAlignment="Center" Height="23"/>
        <Button Content="Зарегестрироваться" HorizontalAlignment="Center" Margin="0,376,0,0" VerticalAlignment="Top" Width="120" Click="click"/>
    </Grid>

</Window>
